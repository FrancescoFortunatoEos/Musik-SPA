{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Your own server",
            "request": "launch",
            "type": "al",
            "environmentType": "OnPrem",    // tipo di ambiente
            "server": "http://bcserver",    // nome del container
            "serverInstance": "BC",     
